<style>
  #shopify-section-{{section.id}}{
    padding-top: 75px;
  }
   #shopify-section-{{section.id}} h2{
    text-align: center;
    color: #B59E5F;
    font-family: "CentraNo2-Bold";
    font-size: 40px;
 
    letter-spacing: 3.5px;
    margin-bottom: 25px;
  }
   #shopify-section-{{section.id}} p{
    text-align: center;
    letter-spacing: 0px;
    color: #FFFFFF;
    font-family: 'CentraNo2-Book' !important;
    font-size: 21px;
    line-height: normal;
    margin: 15px auto 0;
    min-height: 75px;
  }
  #shopify-section-{{section.id}} .yas_reborn-item-wrapp{
    display: flex;
    align-items: center;
    justify-content: center;
    padding: 90px 0 80px;
    position: relative;
  }
 
  #shopify-section-{{section.id}} .yas_reborn-item-wrapp > svg{
    margin: 0 20px;
  }
  #shopify-section-{{section.id}} .yas_reborn_item{
    width: 264px;
    text-align: center;
    display: flex;
    flex-wrap: wrap;
    justify-content: center;
    align-items: flex-start;
  }
    #shopify-section-{{section.id}} .yas_reborn_logo{
   display: none;
  }
   #shopify-section-{{section.id}} .yas_reborn_item h5{
    text-align: center;
    color: #FFFFFF;
    font-family: "CentraNo2-Bold";
    font-size: 25px;
    line-height: 30px; 
    letter-spacing: 0px;
    margin-bottom: 25px;
  }
  #shopify-section-{{section.id}} .yas_reborn_item .yas_reborn_item-img{
    width: 196px;
    height: 196px;
    display: flex;
    align-items: center;
    justify-content: center;
    text-align: center;
  }
  @media (max-width: 1400px){
    #shopify-section-{{section.id}} p{
      font-size: 2.4rem;
    }
  }
  @media (max-width: 1280px){
    
    #shopify-section-{{section.id}} .yas_reborn_item{
      width: 150px;
      justify-content: flex-start;
      flex-direction: column;
      align-items: center;
    }
     #shopify-section-{{section.id}} .yas_reborn_item h5{
       font-size: 25px;
        margin-bottom: 0;
     }
    #shopify-section-{{section.id}} .yas_reborn_item p{
      font-size: 18px;
      line-height: 20px;
    }
    #shopify-section-{{section.id}} .yas_reborn_item .yas_reborn_item-img{
      width: 119px;
      height: 119px;
    }
    #shopify-section-{{section.id}} .yas_reborn_item .yas_reborn_item-img svg{
      height: 65px;
    }
  }
  @media (max-width: 767px){
     #shopify-section-{{section.id}} .yas_reborn_logo{
     display: block;
        
     }
     #shopify-section-{{section.id}} .yas_reborn_logo .yas_reborn_item-img {
     width: 100%;
       
     }
    #shopify-section-{{section.id}} .yas_reborn-item-wrapp{
      flex-wrap: wrap;
      padding: 50px 0 90px;
    }  
    
    #shopify-section-{{section.id}} h2{
      font-size: 35px;
    }
    #shopify-section-{{section.id}} p{
      font-size: 15px;
      line-height: 20px;
      max-width: 280px;
    }
    #shopify-section-{{section.id}} p br{
      display: none;
    }
    #shopify-section-{{section.id}} .yas_reborn-item-wrapp > svg{
      display: none;
    }
  }
</style>
